{
  "name": "monitor_autostart",
  "version": "1.0.4",
  "description": "Monitor Facial IDFace - Sistema de Controle",
  "license": "ISC",
  "author": "Daniel do nascimento",
  "type": "commonjs",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "chcp 65001 && nodemon --signal SIGINT index.js | npx pino-pretty",
    "clean": "if exist dist rmdir /s /q dist",
    "prebuild": "npm run clean && mkdir dist",
    "build": "caxa --input . --output \"dist/MonitorIDFace_autostart.exe\" -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/index.js\"",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mysql2": "^3.14.4",
    "nodemon": "^3.1.10",
    "open": "^10.2.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}